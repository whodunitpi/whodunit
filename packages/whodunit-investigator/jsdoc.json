{
  "source": {
    "include": ["readme.md", "lib"],
    "includePattern": ".+\\.js(doc)?$"
  },
  "opts": {
    "recurse": true,
    "destination": "../whodunit-investigator-doc",
    "template": "node_modules/tui-jsdoc-template",
    "package": "package.json"
  },
  "templates": {
    "logo": {
        "url": "https://raw.githubusercontent.com/whodunit/whodunit.github.io/source/app/assets/img/logo.png",
        "width": "127px",
        "height": "14px"
    },
    "name": "Investigator",
    "footerText": "BSD license Copyright (c) Google"
  },
  "plugins": [
    "plugins/markdown"
  ]
}
